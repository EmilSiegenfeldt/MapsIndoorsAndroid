<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="48dp"

	android:focusable="false"
	android:gravity="center_vertical"
	android:minHeight="48dp"
	android:paddingEnd="16dp"
	android:paddingStart="16dp"
	>

	<FrameLayout
		android:id="@+id/cat_icons"
		android:layout_width="wrap_content"
		android:layout_height="match_parent"
		android:layout_gravity="center_vertical"
		>

		<ImageView
			android:id="@+id/cat_iconitem"
			android:layout_width="34dp"
			android:layout_height="34dp"
			android:layout_gravity="center"
			android:visibility="visible"

		    tools:background="@drawable/misdk_circle_shape"
			/>


		<ImageView
			android:id="@+id/cat_iconitem_tint"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:scaleType="fitCenter"
			android:tint="@color/colorPrimary"
			android:visibility="invisible"
			tools:src="@drawable/ic_human_male_female_24dp"
			/>
	</FrameLayout>

	<TextView
		android:id="@+id/cat_textitem"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_toEndOf="@+id/cat_icons"
		android:layout_marginEnd="16dp"
		android:layout_marginStart="16dp"
		android:gravity="start|center_vertical"
		android:textAlignment="textStart"
		android:textAppearance="@style/TextAppearance.AppCompat.Medium"
		android:textColor="@color/black"
		tools:text="Multiline\ntext\nbla\nbla"
		/>
</RelativeLayout>

